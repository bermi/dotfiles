[ui]
username = <%= print("Mercurial Username: "); STDOUT.flush; STDIN.gets.chomp %> <<%= print("Mercurial email: "); STDOUT.flush; STDIN.gets.chomp %>>

[extensions]
bookmarks =
convert=
fetch=
mq =
patchbomb =
rebase = 
transplant=

[bookmarks]
track.current = True

[diff]
git = True

[extdiff]
cmd.diffmerge = /Applications/DiffMerge.app/Contents/MacOS/DiffMerge

[merge-tools]
diffmerge.executable = /Applications/DiffMerge.app/Contents/MacOS/DiffMerge
diffmerge.args = --result=$output -t1="Local Version" -t2=$output -t3="Other Version" --caption=$output Â $local $base $other
diffmerge.binary = False
diffmerge.symlinks = False
diffmerge.gui = True
diffmerge.premerge = True


[trusted]
groups = web
